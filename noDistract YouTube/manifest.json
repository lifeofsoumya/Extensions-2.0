{
    "name": "No Distract YouTube",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "This extension hides all the unnecessary videos from your YouTube",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://youtube.com/"
    ]
}