{
    "name": "No Distract YouTube",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "This extension hides all the unnecessary videos from your YouTube",
    "icons": { "64": "./icons/iconYt.png"},
    "action": {"default_popup":"popup.html"},
    "background": {
        "service_worker": "popup.js",
        "matches": "*//www.youtube.com/*"
    },
    "permissions": [
        "activeTab"
    ]
}